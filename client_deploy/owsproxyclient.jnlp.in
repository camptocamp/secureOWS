<?xml version="1.0" encoding="utf-8"?>
<!-- JNLP File for OwsProxyClient -->
<jnlp
  spec="1.0+"
  codebase="http://www.secureows.org/securewms/webstart/"
  href="owsproxyclient-@VERSION_JNLP@.jnlp">
  <information>
    <title>Secure WMS Client</title>
    <vendor>Camptocamp SA</vendor>
    <description>Secure WMS Client</description>
    <offline-allowed/>
    <shortcut online="false">
      <desktop/>
      <menu submenu="GIS Tools"/>
    </shortcut>
  </information>
  <security>
      <all-permissions/>
  </security>
  <resources>
    <j2se version="1.5+"/>
    <jar href="owsproxyclient-@VERSION@.jar"/>
  </resources>
  <application-desc main-class="com.camptocamp.owsproxy.OWSClient"/>
</jnlp> 

